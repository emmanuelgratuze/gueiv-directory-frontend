    "precommit": "lint-staged",
    "prettier-js": "prettier-eslint --write './**/*.{ts,tsx,js,jsx}'",
    "prettier-styles": "prettier-stylelint --write './**/*.js'"
  },
  "lint-staged": {
    "./**/*.js": "npm run lint"
  },
  "dependencies": {
