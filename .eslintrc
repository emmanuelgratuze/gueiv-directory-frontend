{
  "extends": ["airbnb"],
  "rules": {
    "semi": 0,
    "comma-dangle": 0,
    "no-mixed-operators": 0,
    "arrow-parens": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-props-no-spreading": "off",
    "max-len": 0
  },
  "env": {
    "browser": true
  },
  "globals": {
    "fetch": true
  }
}