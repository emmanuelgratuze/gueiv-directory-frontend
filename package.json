{
  "name": "gueiv-directory-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run pre-build && next",
    "build": "next build",
    "production-build": "NODE_ENV=production npm run pre-build && npm run build && npm run export",
    "pre-build": "babel-node -r dotenv/config ./cms/build.js --presets @babel/preset-env",
    "start": "next start",
    "export": "next export",
    "clean-babel-cache": "rm -Rf ./node_modules/.cache/babel-loader/*",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:stylelint": "stylelint './**/*.js' --fix",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .eslintignore",
    "postinstall": "# cp -r vendor/gsap/bonus-files-for-npm-users/* node_modules/gsap/",
    "precommit": "lint-staged",
    "prettier-js": "prettier-eslint --write './**/*.{ts,tsx,js,jsx}'",
    "prettier-styles": "prettier-stylelint --write './**/*.{ts,tsx,js,jsx}'"
  },
  "lint-staged": {
    "./**/*.js": "npm run lint"
  },
  "dependencies": {
    "cloudinary": "^1.21.0",
    "cloudinary-core": "^2.8.2",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "es6-promise": "^4.2.8",
    "fetch-jsonp": "^1.1.3",
    "find-cache-dir": "^3.3.1",
    "framer-motion": "^1.10.3",
    "grommet": "https://github.com/grommet/grommet/tarball/stable",
    "grommet-icons": "^4.4.0",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.15",
    "netlify-cms": "^2.10.39",
    "netlify-cms-core": "^2.23.3",
    "next": "^9.3.4",
    "next-absolute-url": "^1.2.2",
    "next-compose-plugins": "^2.2.0",
    "next-images": "^1.4.0",
    "next-transpile-modules": "^3.2.0",
    "node-fetch": "^2.6.0",
    "normalizr": "^3.6.0",
    "polished": "^3.5.1",
    "prop-types": "^15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-ga": "^2.7.0",
    "react-infinite-scroll-component": "github:emmanuelgratuze/react-infinite-scroll-component",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-api-middleware": "^3.2.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-immutable": "^4.0.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-multi": "^0.1.12",
    "redux-persist": "^6.0.0",
    "request": "^2.88.2",
    "reselect": "^4.0.0",
    "styled-components": "^5.1.0",
    "throttle-debounce": "^2.1.0",
    "webpack": "^4.42.1",
    "yaml": "^1.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@svgr/webpack": "^5.3.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.0",
    "@types/node-fetch": "^2.5.5",
    "@types/react": "^16.9.32",
    "@types/react-redux": "^7.1.7",
    "@types/redux": "^3.6.0",
    "@types/redux-api-middleware": "^3.0.7",
    "@types/redux-immutable": "^4.0.1",
    "@types/redux-localstorage": "^1.0.8",
    "@types/redux-saga": "^0.10.5",
    "@types/request": "^2.48.4",
    "@types/styled-components": "^5.0.1",
    "@types/throttle-debounce": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-typescript-to-proptypes": "^1.3.2",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-module-resolver": "^0.16.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "fetch-jsonp-polyfill": "^1.1.5",
    "lint-staged": "^9.5.0",
    "path": "^0.12.7",
